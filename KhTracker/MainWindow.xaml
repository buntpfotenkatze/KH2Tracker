                        <ColumnDefinition Width="0.05*" />

                        <ColumnDefinition Width="1.35*" />
                        <ColumnDefinition Width="0.05*" />

                        <ColumnDefinition Width="1.35*" />
                        <ColumnDefinition Width="0.05*" />

                        <ColumnDefinition Width="1.35*" />
                        <ColumnDefinition Width="0.05*" />

                        <!--<ColumnDefinition Width="1*" />
                        <ColumnDefinition Width="0.2*" />
                        <ColumnDefinition Width="1.5*" />-->
                        <ColumnDefinition Width="0*" />
                        <ColumnDefinition Width="0*" />
                        <ColumnDefinition Width="0*" />

                        <ColumnDefinition Width="1*" />
                        <ColumnDefinition Width="0.2*" />
                        <ColumnDefinition Width="1.5*" />
                    </Grid.ColumnDefinitions>
                    <!-- Level -->
                    <Grid x:Name="Level" Visibility="Visible">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="0.6*" />
                            <ColumnDefinition Width="1*" />
                        </Grid.ColumnDefinitions>
                        <ContentControl Content="{DynamicResource LevelIcon}" x:Name="LevelIcon" />
                        <Viewbox Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Stretch"
                                 Margin="0,0,0,2">
                            <local:OutlinedTextBlock x:Name="LevelValue" FontFamily="{StaticResource Gummi}"
                                                     FontSize="20" FontStretch="Medium"
                                                     AllowDrop="False" IsHitTestVisible="False" StrokeThickness="1"
                                                     Text="99">
                                <local:OutlinedTextBlock.Fill>
                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop Color="#827D7D" Offset="1" />
                                        <GradientStop Color="#F1E3E3" Offset="0.4" />
                                    </LinearGradientBrush>
                                </local:OutlinedTextBlock.Fill>
                            </local:OutlinedTextBlock>
                        </Viewbox>
