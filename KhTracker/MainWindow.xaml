                    </Image>
                    <Image x:Name="Cross11" Source="Images/System/cross.png" Visibility="Collapsed" Grid.Column="10"
                           Panel.ZIndex="-2" Opacity="1">
                        <Image.Effect>
                            <fx:BrightnessContrastEffect Brightness="-1" />
                        </Image.Effect>
                    </Image>
                </Grid>
            </Grid>

            <!-- Setting Visuals -->
            <Grid Grid.Row="4">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="0.33*" />
                    <ColumnDefinition />
                    <ColumnDefinition Width="0.01*" />
                    <ColumnDefinition Width="0.1*" />
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="0.1*" />
                    <RowDefinition />
                </Grid.RowDefinitions>

                <!-- Text -->
                <Viewbox Grid.Row="1">
                    <local:OutlinedTextBlock x:Name="SettingsText" FontFamily="{StaticResource Gummi}"
                                             FontSize="10pt" Margin="4,0,0,2"
                                             FontWeight="DemiBold" StrokeThickness="3" Stroke="Black" Text="Settings:">
                        <local:OutlinedTextBlock.Fill>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FF7B7B7B" Offset="1" />
                                <GradientStop Color="#FFE2ECFF" Offset="0.655" />
                            </LinearGradientBrush>
                        </local:OutlinedTextBlock.Fill>
                    </local:OutlinedTextBlock>
                </Viewbox>

                <!-- Setting Icons -->
                <Grid Grid.Row="1" Grid.Column="1">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Name="SettingLevel01" Width="1.5*" />
                        <ColumnDefinition Name="SettingLevel50" Width="0*" />
                        <ColumnDefinition Name="SettingLevel99" Width="0*" />
                        <ColumnDefinition Name="SettingBetterStt" Width="1.1*" />
                        <ColumnDefinition Name="SettingAbsent" Width="0*" />
                        <ColumnDefinition Name="SettingAbsentSplit" Width="1.1*" />
                        <ColumnDefinition Name="SettingDatas" Width="1*" />
                        <ColumnDefinition Name="SettingSephiroth" Width="1*" />
                        <ColumnDefinition Name="SettingTerra" Width="1*" />
                        <ColumnDefinition Name="SettingCups" Width="1*" />
                        <ColumnDefinition Name="SettingHadesCup" Width="1*" />
                        <ColumnDefinition Name="SettingCavern" Width="1*" />
                        <ColumnDefinition Name="SettingTransport" Width="1*" />

                        <!-- +1.0 for each setting off (level1/50/99 counted together as 1 is any of them are on) -->
                        <ColumnDefinition Name="SettingSpacer" Width="0*" />

                        <ColumnDefinition Width="1.25*" />
                        <ColumnDefinition Width="0.25*" />
                    </Grid.ColumnDefinitions>

                    <Image Grid.Column="0" Source="Images/System/settings/Level_01.png" ToolTip="Level 1 Checks" />
                    <Image Grid.Column="1" Source="Images/System/settings/Level_50.png" ToolTip="Level 50 Checks" />
                    <Image Grid.Column="2" Source="Images/System/settings/Level_99.png" ToolTip="Level 99 Checks" />
                    <Image Grid.Column="3" Source="Images/System/settings/better_stt.png"
                           ToolTip="Roxas STT Improvements" Margin="0,-4,0,-4" />
                    <Image Grid.Column="4" Source="Images/System/settings/absent_silhouette.png"
                           ToolTip="Absent Silhouettes" />
                    <Image Grid.Column="5" Source="Images/System/settings/absent_silhouette_split.png"
                           ToolTip="Absent Silhouettes (Data Split on)" />
                    <Image Grid.Column="6" Source="Images/System/settings/data_organization.png"
                           ToolTip="Data Organization" />
                    <Image Grid.Column="7" Source="Images/System/settings/sephiroth.png" ToolTip="Sephiroth" />
                    <Image Grid.Column="8" Source="Images/System/settings/lingering_will.png"
                           ToolTip="Lingering Will (Terra)" />
                    <Image Grid.Column="9" Source="Images/System/settings/underdrome_cups.png"
                           ToolTip="Underdrome Cups" />
                    <Image Grid.Column="10" Source="Images/System/settings/paradox_cup.png" ToolTip="Hades Paradox Cup" />
                    <Image Grid.Column="11" Source="Images/System/settings/cavern_of_remembrance.png"
                           ToolTip="Cavern of Rememberance" />
                    <Image Grid.Column="12" Source="Images/System/settings/transport.png"
                           ToolTip="Transport to Rememberance" />

                </Grid>

                <!-- Auto-Detect Visual -->
                <Image Grid.Row="1" RenderTransformOrigin="0.5,0.5" x:Name="Connect"
                       Source="Images/System/config/searching.png" Grid.Column="3" Margin="0,3" Visibility="Collapsed">
                    <Image.RenderTransform>
                        <RotateTransform Angle="0" />
                    </Image.RenderTransform>
                    <Image.Style>
                        <Style>
                            <Style.Triggers>
                                <Trigger Property="Image.Visibility" Value="Visible">
                                    <Trigger.EnterActions>
                                        <BeginStoryboard>
                                            <Storyboard>
                                                <DoubleAnimation
                                                    Storyboard.TargetProperty="RenderTransform.Angle"
                                                    From="0"
                                                    To="360"
                                                    Duration="0:0:2"
                                                    RepeatBehavior="Forever" DecelerationRatio="0.75" />
                                            </Storyboard>
                                        </BeginStoryboard>
                                    </Trigger.EnterActions>
                                </Trigger>
                            </Style.Triggers>
                        </Style>
                    </Image.Style>
                </Image>

                <Image Grid.Row="1" x:Name="Connect2" Source="Images/System/config/searching.png" Grid.Column="3"
                       Margin="0,3" Visibility="Collapsed" />

                <!-- fancy spacer colors -->
                <Rectangle Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="2" Fill="#D81E1E1E" Panel.ZIndex="-1" />
                <Rectangle Grid.Row="1" Grid.Column="2" Fill="Black" Panel.ZIndex="-1" />
                <Rectangle Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="4" Fill="Black" Panel.ZIndex="-1" />
                <Rectangle Grid.Row="1" Grid.Column="3" Fill="#D8282828" Panel.ZIndex="-1" />
                <!--Rectangle Margin="-6,0,44,0" Grid.Column="3" Fill="#ff000000" Panel.ZIndex="2"/-->
            </Grid>
        </Grid>

    </DockPanel>
</Window>