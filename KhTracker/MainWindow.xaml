<Window
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="clr-namespace:KhTracker"
    xmlns:fx="http://usercode.de/wpf.imageeffects" x:Class="KhTracker.MainWindow"
    mc:Ignorable="d"
    ResizeMode="CanResize"
    KeyDown="Window_KeyDown"
    Closing="Window_Closing"
    LocationChanged="Window_LocationChanged"
    SizeChanged="Window_SizeChanged"
    AllowDrop="True" Drop="DropFile"
    Title="KH2 Tracker 2.30" Height="880" Width="570" Background="#303030"
    MinHeight="440" MinWidth="300">

    <Window.Resources>
        <Style TargetType="{x:Type Image}">
            <Setter Property="RenderOptions.BitmapScalingMode" Value="HighQuality" />
        </Style>
    </Window.Resources>

    <DockPanel>
        <Menu DockPanel.Dock="Top" Background="White">
            <Menu.ItemsPanel>
                <ItemsPanelTemplate>
                    <DockPanel HorizontalAlignment="Stretch" />
                </ItemsPanelTemplate>
            </Menu.ItemsPanel>
            <MenuItem Header="Options" Focusable="False">
                <MenuItem Header="Load Hints...">
                    <MenuItem Header="Load Archipelago Settings (.yaml)" Click="LoadArchipelagoSettings" />
                </MenuItem>
                <Separator />
                <MenuItem Header="Start Auto-Tracking" Click="InitTracker" x:Name="TrackToggle" />
                <MenuItem Header="Enable Auto-Connect/Reconnect" Click="AutoConnectToggle" x:Name="AutoConnectOption"
                          IsCheckable="True" IsChecked="False"
                          ToolTip="Automatically search and connect to the game on tracker start-up or if game closes." />
                <Separator />
                <MenuItem Header="Save Current Progress" Click="SaveProgress" />
                <MenuItem Header="Load Tracker Progress" Click="LoadProgress" />
                <MenuItem Header="Auto-Save Tracker Progress on Tracker Exit" Click="AutoSaveProgressToggle"
                          x:Name="AutoSaveProgressOption" IsCheckable="True" IsChecked="False" />
                <MenuItem Header="Auto-Save Tracker Progress on Connection Loss" Click="AutoSaveProgress2Toggle"
                          x:Name="AutoSaveProgress2Option" IsCheckable="True" IsChecked="False" />
                <Separator />
                <MenuItem Header="Reset Tracker" Click="OnReset" />
                <MenuItem Header="Always on Top" Click="TopMostToggle" x:Name="TopMostOption" IsCheckable="True"
                          IsChecked="False" />
                <MenuItem Header="Drag and Drop" Click="DragDropToggle" x:Name="DragAndDropOption" IsCheckable="True"
                          IsChecked="True" />
                <MenuItem Header="Emit proof keystroke" Click="EmitProofKeystrokeToggle"
                          x:Name="EmitProofKeystrokeOption" IsCheckable="True" IsChecked="True" />
                <Separator />
                <MenuItem Header="Reset Window Size" Click="ResetSize" />
                <MenuItem Header="Show Disconnect Message" Click="DisconnectToggle" x:Name="Disconnect"
                          IsCheckable="True" IsChecked="True" />
            </MenuItem>
            <MenuItem Header="Display Toggles" Focusable="False">
                <MenuItem Header="Lucky Emblems" Click="LuckyEmblemsToggle" x:Name="LuckyEmblemsOption" IsCheckable="True"
                          IsChecked="True" />
                <MenuItem Header="Torn Pages" Click="TornPagesToggle" x:Name="TornPagesOption" IsCheckable="True"
                          IsChecked="True" />
                <MenuItem Header="Once More / Second Chance" Click="AbilitiesToggle" x:Name="AbilitiesOption"
                          IsCheckable="True" IsChecked="True" />
                <MenuItem Header="Anti Form" Click="AntiFormToggle" x:Name="AntiFormOption" IsCheckable="True"
                          IsChecked="False" />
                <MenuItem Header="World Lock Checks" Click="VisitLockToggle" x:Name="VisitLockOption"
                          IsCheckable="True" IsChecked="True" />
                <MenuItem Header="Extra Important Checks" Click="ExtraChecksToggle" x:Name="ExtraChecksOption"
                          IsCheckable="True" IsChecked="True" />
                <Separator />
                <MenuItem Header="Show Seed Hash" Click="SeedHashToggle" x:Name="SeedHashOption" IsCheckable="True"
                          IsChecked="True" />
                <MenuItem Header="Show World Progression Icons" Click="WorldProgressToggle"
                          x:Name="WorldProgressOption" IsCheckable="True" IsChecked="True" />
                <MenuItem Header="Show Form and Growth Levels" Click="FormsGrowthToggle" x:Name="FormsGrowthOption"
                          IsCheckable="True" IsChecked="True" />
                <MenuItem Header="Show Death Counter" Click="DeathCounterToggle" x:Name="DeathCounterOption"
                          IsCheckable="True" IsChecked="False" />
                <MenuItem Header="Highlight Current World" Click="WorldHighlightToggle" x:Name="WorldHighlightOption"
                          IsCheckable="True" IsChecked="True" />
                <Separator />
                <MenuItem Header="Show Next Level Check Display" Click="NextLevelCheckToggle"
                          x:Name="NextLevelCheckOption" IsCheckable="True" IsChecked="False" />
                <MenuItem Header="Level 1 Checks" Click="SoraLevel01Toggle" x:Name="SoraLevel01Option"
                          IsCheckable="True" IsChecked="False" />
                <MenuItem Header="Level 50 Checks" Click="SoraLevel50Toggle" x:Name="SoraLevel50Option"
                          IsCheckable="True" IsChecked="True" />
                <MenuItem Header="Level 99 Checks" Click="SoraLevel99Toggle" x:Name="SoraLevel99Option"
                          IsCheckable="True" IsChecked="False" />
            </MenuItem>
            <MenuItem Header="Image Visuals" Focusable="False">
                <!-- eventually add an ption that pulls up settings window... -->
                <!--MenuItem Header="Simple Stat/UI Icons" Click="MinStatToggle" x:Name="MinStatOption" IsCheckable="True" IsChecked="True"/>
                <MenuItem Header="Game Stat/UI Icons"   Click="OldStatToggle" x:Name="OldStatOption" IsCheckable="True" IsChecked="False"/>
                <Separator/-->
                <MenuItem Header="New World Layout" Click="NewWorldLayoutToggle" x:Name="NewWorldLayoutOption"
                          IsCheckable="True" IsChecked="True" />
                <MenuItem Header="Classic World Layout" Click="OldWorldLayoutToggle" x:Name="OldWorldLayoutOption"
                          IsCheckable="True" IsChecked="False" />
                <Separator />
                <MenuItem Header="Colorful Hint Text" Click="ColorHintToggle" x:Name="ColorHintOption"
                          IsCheckable="True" IsChecked="True" />
                <Separator />
                <MenuItem Header="Simple World Icons" Click="MinWorldToggle" x:Name="MinWorldOption" IsCheckable="True"
                          IsChecked="True" />
                <MenuItem Header="Game World Icons" Click="OldWorldToggle" x:Name="OldWorldOption" IsCheckable="True"
                          IsChecked="False" />
                <Separator />
                <MenuItem Header="Simple Check Icons" Click="MinCheckToggle" x:Name="MinCheckOption" IsCheckable="True"
                          IsChecked="True" />
                <MenuItem Header="Game Check Icons" Click="OldCheckToggle" x:Name="OldCheckOption" IsCheckable="True"
                          IsChecked="False" />
                <Separator />
                <MenuItem Header="Simple Progression Icons" Click="MinProgToggle" x:Name="MinProgOption"
                          IsCheckable="True" IsChecked="True" />
                <MenuItem Header="Game Progression Icons" Click="OldProgToggle" x:Name="OldProgOption"
                          IsCheckable="True" IsChecked="False" />
                <Separator />
                <MenuItem Header="Load Custom Images" Click="CustomImageToggle" x:Name="CustomFolderOption"
                          IsCheckable="True" IsChecked="False" />
                <Separator />
                <MenuItem Header="Main Window Background">
                    <MenuItem x:Name="MainDefOption" Header="Default" IsCheckable="True" IsChecked="True"
                              Click="MainBG_DefToggle" />
                    <MenuItem x:Name="MainImg1Option" Header="Custom Image (Stretch)" IsCheckable="True"
                              IsChecked="False" Click="MainBG_Img1Toggle" />
                    <MenuItem x:Name="MainImg2Option" Header="Custom Image (Uniform)" IsCheckable="True"
                              IsChecked="False" Click="MainBG_Img2Toggle" />
                    <MenuItem x:Name="MainImg3Option" Header="Custom Image (No Scaling)" IsCheckable="True"
                              IsChecked="False" Click="MainBG_Img3Toggle" />
                </MenuItem>
            </MenuItem>
            <MenuItem Header="Worlds" Focusable="False">
                <MenuItem Header="Sora's Heart" Click="SoraHeartToggle" x:Name="SoraHeartOption" IsCheckable="True"
                          IsChecked="True" />
                <MenuItem Header="Drive Forms" Click="DrivesToggle" x:Name="DrivesOption" IsCheckable="True"
                          IsChecked="True" />
                <MenuItem Header="Simulated Twilight Town" Click="SimulatedToggle" x:Name="SimulatedOption"
                          IsCheckable="True" IsChecked="True" />
                <MenuItem Header="Twilight Town" Click="TwilightTownToggle" x:Name="TwilightTownOption"
                          IsCheckable="True" IsChecked="True" />
                <MenuItem Header="Hollow Bastion" Click="HollowBastionToggle" x:Name="HollowBastionOption"
                          IsCheckable="True" IsChecked="True" />
                <MenuItem Header="Beast's Castle" Click="BeastCastleToggle" x:Name="BeastCastleOption"
                          IsCheckable="True" IsChecked="True" />
                <MenuItem Header="Olympus Coliseum" Click="OlympusToggle" x:Name="OlympusOption" IsCheckable="True"
                          IsChecked="True" />
                <MenuItem Header="Agrabah" Click="AgrabahToggle" x:Name="AgrabahOption" IsCheckable="True"
                          IsChecked="True" />
                <MenuItem Header="Land of Dragons" Click="LandofDragonsToggle" x:Name="LandofDragonsOption"
                          IsCheckable="True" IsChecked="True" />
                <MenuItem Header="Disney Castle" Click="DisneyCastleToggle" x:Name="DisneyCastleOption"
                          IsCheckable="True" IsChecked="True" />
                <MenuItem Header="Pride Lands" Click="PrideLandsToggle" x:Name="PrideLandsOption" IsCheckable="True"
                          IsChecked="True" />
                <MenuItem Header="Port Royal" Click="PortRoyalToggle" x:Name="PortRoyalOption" IsCheckable="True"
                          IsChecked="True" />
                <MenuItem Header="Halloween Town" Click="HalloweenTownToggle" x:Name="HalloweenTownOption"
                          IsCheckable="True" IsChecked="True" />
                <MenuItem Header="Space Paranoids" Click="SpaceParanoidsToggle" x:Name="SpaceParanoidsOption"
                          IsCheckable="True" IsChecked="True" />
                <MenuItem Header="The World that Never Was" Click="TwtnwToggle" x:Name="TwtnwOption" IsCheckable="True"
                          IsChecked="True" />
                <MenuItem Header="100 Acre Wood" Click="HundredAcreWoodToggle" x:Name="HundredAcreWoodOption"
                          IsCheckable="True" IsChecked="True" />
                <MenuItem Header="Atlantica" Click="AtlanticaToggle" x:Name="AtlanticaOption" IsCheckable="True"
                          IsChecked="True" />
                <MenuItem Header="Synthesis" Click="SynthToggle" x:Name="SynthOption" IsCheckable="True"
                          IsChecked="True" />
                <MenuItem Header="Puzzles" Click="PuzzleToggle" x:Name="PuzzleOption" IsCheckable="True"
                          IsChecked="True" />
            </MenuItem>
            <MenuItem x:Name="ModeDisplay" Header=" " Focusable="False" IsHitTestVisible="False"
                      HorizontalAlignment="Right" />
            <!--MenuItem Header="-ppk 1.6.8-" IsHitTestVisible="False" Focusable="False" HorizontalAlignment="Right"/-->
        </Menu>

        <Grid>
            <Grid.RowDefinitions>
                <!-- 6.6 -->
                <RowDefinition x:Name="WorldRow" Height="6.6*" />
                <!-- 0.8 -->
                <RowDefinition x:Name="InfoRow" Height="0.8*" />
                <!-- 0.45 -->
                <RowDefinition x:Name="FormRow" Height="0.45*" />
                <!-- 2.2 -->
                <RowDefinition x:Name="CheckRow" Height="2.2*" />
                <!-- 0.5 -->
                <RowDefinition x:Name="SettingRow" Height="0.5*" />
            </Grid.RowDefinitions>

            <!-- World Grids -->
            <Grid Grid.Row="0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition />
                    <ColumnDefinition />
                </Grid.ColumnDefinitions>

                <!-- Left Side -->
                <Grid Grid.Column="0" Name="WorldsLeft">
                    <Grid.RowDefinitions>
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                    </Grid.RowDefinitions>

                    <!-- Sora's Heart -->
                    <Grid x:Name="SorasHeartTop" Grid.Row="0">
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition Height="0.04*" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="0.27*" />
                            <!-- Path Width 0.1 when on -->
                            <ColumnDefinition Width="0.*" />
                            <ColumnDefinition Width="0.73*" />
                        </Grid.ColumnDefinitions>
                        <!-- Main Icons -->
                        <Grid Grid.Column="0">
                            <Grid.RowDefinitions>
                                <RowDefinition />
                                <RowDefinition Height="0.65*" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition />
                                <ColumnDefinition />
                                <ColumnDefinition Width="0.5*" />
                                <ColumnDefinition Width="0.5*" />
                            </Grid.ColumnDefinitions>
                            <!-- main icon -->
                            <Button Grid.RowSpan="2" Grid.ColumnSpan="3" x:Name="SorasHeart"
                                    PreviewMouseDown="OnMouseDown" MouseWheel="OnMouseWheel"
                                    Template="{StaticResource DefaultButton}" FocusVisualStyle="{x:Null}"
                                    Content="{DynamicResource Min-SoraHeartImage}" />
                            <!-- cross icon -->
                            <Image Grid.Row="0" Grid.Column="0" Grid.RowSpan="2" Grid.ColumnSpan="3"
                                   x:Name="SorasHeartCross" Source="Images/System/crossworld.png"
                                   Visibility="Collapsed" IsHitTestVisible="False" RenderTransformOrigin="0.5,0.5"
                                   Opacity="0.9" Panel.ZIndex="2">
                                <Image.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform ScaleX="1.2" ScaleY="1.2" />
                                    </TransformGroup>
                                </Image.RenderTransform>
                            </Image>
                            <!-- question mark -->
                            <Image Grid.Row="0" Grid.Column="0" Grid.RowSpan="2" Grid.ColumnSpan="3"
                                   x:Name="SorasHeartQuestion" Source="Images/System/question.png"
                                   Visibility="Collapsed" IsHitTestVisible="False" RenderTransformOrigin="0.5,0.5"
                                   Opacity="0.9" Panel.ZIndex="2">
                                <Image.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform ScaleX="1.2" ScaleY="1.2" />
                                    </TransformGroup>
                                </Image.RenderTransform>
                            </Image>
                            <!-- world value -->
                            <Viewbox Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="3" Panel.ZIndex="3" Margin="0,2,5,0"
                                     HorizontalAlignment="Right" MaxHeight="25" VerticalAlignment="Top">
                                <local:OutlinedTextBlock x:Name="SorasHeartHint" FontFamily="{StaticResource Gummi}"
                                                         FontSize="25pt"
                                                         StrokeThickness="7" Fill="#FFEFECE9" Text="?"
                                                         Margin="0,-5,0,2" />
                            </Viewbox>
                            <!-- progression -->
                            <Image Name="SorasHeartWeapon" Panel.ZIndex="1" IsHitTestVisible="False" Grid.Row="0"
                                   Grid.Column="2" Grid.ColumnSpan="2" HorizontalAlignment="Left" />
                        </Grid>
                        <!-- path hints icons -->
                        <Grid x:Name="SorasHeartPath" Grid.Row="0" Grid.Column="1">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="0.18*" />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition Height="0.18*" />
                            </Grid.RowDefinitions>
                            <Image x:Name="SorasHeartPathCon" Source="Images/System/proof_connection.png"
                                   Visibility="Visible" Grid.Row="1" VerticalAlignment="Bottom" />
                            <Image x:Name="SorasHeartPathNon" Source="Images/System/proof_nonexistence.png"
                                   Visibility="Visible" Grid.Row="2" />
                            <Image x:Name="SorasHeartPathPea" Source="Images/System/proof_peace.png"
                                   Visibility="Visible" Grid.Row="3" VerticalAlignment="Top" />
                        </Grid>
                        <!-- item grid -->
                        <local:WorldGrid x:Name="SorasHeartGrid" Grid.Row="0" Grid.Column="2" />
                        <!-- fancy colors -->
                        <Rectangle Grid.Row="0" Grid.Column="0" Fill="#D81E1E1E" Panel.ZIndex="-1" />
                        <Rectangle Grid.Row="0" Grid.Column="1" Fill="#FF454545" Panel.ZIndex="-1" Opacity="0.9" />
                        <Rectangle Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="2" Fill="#D81E1E1E" Panel.ZIndex="-1" />
                        <Rectangle Name="SorasHeartSelWg" Grid.Row="0" Grid.Column="2" Fill="#3F505080"
                                   Panel.ZIndex="-1" Visibility="Collapsed" />
                    </Grid>

                    <!-- Simulated Twilight Town -->
                    <Grid x:Name="SimulatedTwilightTownTop" Grid.Row="1">
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition Height="0.04*" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="0.27*" />
                            <!-- Path Width 0.1 when on -->
                            <ColumnDefinition Width="0.0*" />
                            <ColumnDefinition Width="0.73*" />
                        </Grid.ColumnDefinitions>
                        <!-- Main Icons -->
                        <Grid Grid.Column="0">
                            <Grid.RowDefinitions>
                                <RowDefinition />
                                <RowDefinition Height="0.65*" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition />
                                <ColumnDefinition />
                                <ColumnDefinition Width="0.5*" />
                                <ColumnDefinition Width="0.5*" />
                            </Grid.ColumnDefinitions>
                            <!-- main icon -->
                            <Button Grid.RowSpan="2" Grid.ColumnSpan="3" x:Name="SimulatedTwilightTown"
                                    Background="Transparent" PreviewMouseDown="OnMouseDown" MouseWheel="OnMouseWheel"
                                    Template="{StaticResource DefaultButton}" FocusVisualStyle="{x:Null}"
                                    Content="{DynamicResource Min-SimulatedImage}" />
                            <!-- cross icon -->
                            <Image Grid.Row="0" Grid.Column="0" Grid.RowSpan="2" Grid.ColumnSpan="3"
                                   x:Name="SimulatedTwilightTownCross" Source="Images/System/crossworld.png"
                                   Visibility="Collapsed" IsHitTestVisible="False" RenderTransformOrigin="0.5,0.5"
                                   Opacity="0.9" Panel.ZIndex="2">
                                <Image.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform ScaleX="1.2" ScaleY="1.2" />
                                    </TransformGroup>
                                </Image.RenderTransform>
                            </Image>
                            <!-- question mark -->
                            <Image Grid.Row="0" Grid.Column="0" Grid.RowSpan="2" Grid.ColumnSpan="3"
                                   x:Name="SimulatedTwilightTownQuestion" Source="Images/System/question.png"
                                   Visibility="Collapsed" IsHitTestVisible="False" RenderTransformOrigin="0.5,0.5"
                                   Opacity="0.9" Panel.ZIndex="2">
                                <Image.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform ScaleX="1.2" ScaleY="1.2" />
                                    </TransformGroup>
                                </Image.RenderTransform>
                            </Image>
                            <!-- lock icons -->
                            <Image Grid.Row="0" Grid.Column="0" x:Name="SimulatedTwilightTownLock"
                                   Source="Images/Worlds/Locks/TT2.png" Grid.RowSpan="2" Grid.ColumnSpan="3"
                                   Panel.ZIndex="1" RenderTransformOrigin="0.5,0.5" IsHitTestVisible="False">
                                <Image.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform ScaleX="0.5" ScaleY="0.5" />
                                    </TransformGroup>
                                </Image.RenderTransform>
                            </Image>
                            <!-- world value -->
                            <Viewbox Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="3" Panel.ZIndex="3" Margin="0,2,5,0"
                                     HorizontalAlignment="Right" MaxHeight="25">
                                <local:OutlinedTextBlock x:Name="SimulatedTwilightTownHint"
                                                         FontFamily="{StaticResource Gummi}" FontSize="25pt"
                                                         StrokeThickness="7" Fill="#FFEFECE9" Text="?"
                                                         Margin="0,-5,0,2" />
                            </Viewbox>
                            <!-- progression -->
                            <ContentControl Content="{StaticResource Min-Axel1}"
                                            x:Name="SimulatedTwilightTownProgression" Panel.ZIndex="1" Grid.Row="0"
                                            Grid.Column="1"
                                            Grid.ColumnSpan="3" HorizontalAlignment="Right" MaxHeight="55" />
                        </Grid>
                        <!-- item grid -->
                        <local:WorldGrid x:Name="SimulatedTwilightTownGrid" Grid.Row="0" Grid.Column="2" />
                        <!-- fancy spacer colors -->
                        <Rectangle Grid.Row="0" Grid.Column="0" Fill="#D81E1E1E" Panel.ZIndex="-1" />
                        <Rectangle Grid.Row="0" Grid.Column="1" Fill="#FF454545" Panel.ZIndex="-1" Opacity="0.9" />
                        <Rectangle Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="2" Fill="#D81E1E1E" Panel.ZIndex="-1" />
                        <Rectangle Name="SimulatedTwilightTownSelWg" Grid.Row="0" Grid.Column="2" Fill="#3F505080"
                                   Panel.ZIndex="-1" Visibility="Collapsed" />
                    </Grid>

                    <!-- Hollow Bastion -->
                    <Grid x:Name="HollowBastionTop" Grid.Row="2">
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition Height="0.04*" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="0.27*" />
                            <!-- Path Width 0.1 when on -->
                            <ColumnDefinition Width="0.0*" />
                            <ColumnDefinition Width="0.73*" />
                        </Grid.ColumnDefinitions>
                        <!-- Main Icons -->
                        <Grid Grid.Column="0">
                            <Grid.RowDefinitions>
                                <RowDefinition />
                                <RowDefinition Height="0.65*" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition />
                                <ColumnDefinition />
                                <ColumnDefinition Width="0.5*" />
                                <ColumnDefinition Width="0.5*" />
                            </Grid.ColumnDefinitions>
                            <!-- main icon -->
                            <Button Opacity="1" Grid.RowSpan="2" Grid.ColumnSpan="3" x:Name="HollowBastion"
                                    Background="Transparent" PreviewMouseDown="OnMouseDown" MouseWheel="OnMouseWheel"
                                    Template="{StaticResource DefaultButton}" FocusVisualStyle="{x:Null}"
                                    Content="{DynamicResource Min-HollowBastionImage}" />
                            <!-- cross icon -->
                            <Image Grid.Row="0" Grid.Column="0" Grid.RowSpan="2" Grid.ColumnSpan="3"
                                   x:Name="HollowBastionCross"
                                   Source="Images/System/crossworld.png" Visibility="Collapsed"
                                   IsHitTestVisible="False" RenderTransformOrigin="0.5,0.5" Opacity="0.9"
                                   Panel.ZIndex="2">
                                <Image.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform ScaleX="1.2" ScaleY="1.2" />
                                    </TransformGroup>
                                </Image.RenderTransform>
                            </Image>
                            <!-- question mark -->
                            <Image Grid.Row="0" Grid.Column="0" Grid.RowSpan="2" Grid.ColumnSpan="3"
                                   x:Name="HollowBastionQuestion"
                                   Source="Images/System/question.png" Visibility="Collapsed" IsHitTestVisible="False"
                                   RenderTransformOrigin="0.5,0.5" Opacity="0.9" Panel.ZIndex="2">
                                <Image.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform ScaleX="1.2" ScaleY="1.2" />
                                    </TransformGroup>
                                </Image.RenderTransform>
                            </Image>
                            <!-- lock icon -->
                            <Image Grid.Row="0" Grid.Column="0" x:Name="HollowBastionLock1"
                                   Source="Images/Worlds/Locks/HB.png"
                                   Grid.RowSpan="2" Grid.ColumnSpan="3" Panel.ZIndex="1" RenderTransformOrigin="0,0.5"
                                   IsHitTestVisible="False">
                                <Image.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform ScaleX="0.5" ScaleY="0.5" />
                                    </TransformGroup>
                                </Image.RenderTransform>
                            </Image>
                            <Image Grid.Row="0" Grid.Column="0" x:Name="HollowBastionLock2"
                                   Source="Images/Worlds/Locks/HB.png"
                                   Grid.RowSpan="2" Grid.ColumnSpan="3" Panel.ZIndex="1" RenderTransformOrigin="1,0.5"
                                   IsHitTestVisible="False">
                                <Image.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform ScaleX="0.5" ScaleY="0.5" />
                                    </TransformGroup>
                                </Image.RenderTransform>
                            </Image>
                            <!-- world value -->
                            <Viewbox Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="3" Panel.ZIndex="3" Margin="0,2,5,0"
                                     HorizontalAlignment="Right" MaxHeight="25">
                                <local:OutlinedTextBlock x:Name="HollowBastionHint" FontFamily="{StaticResource Gummi}"
                                                         FontSize="25pt"
                                                         StrokeThickness="7" Fill="#FFEFECE9" Text="?"
                                                         Margin="0,-5,0,2" />
                            </Viewbox>
                            <!-- progression -->
                            <ContentControl Grid.Row="0" x:Name="HollowBastionProgression" Panel.ZIndex="1"
                                            Grid.Column="2" Grid.ColumnSpan="2" HorizontalAlignment="Left" />
                            <!--Image Panel.ZIndex="1" Grid.Column="2" Grid.ColumnSpan="2" HorizontalAlignment="Left" Source="Images/default_progression_test/corridors.png" Margin="-5,0,0,0"/-->
                        </Grid>
                        <!-- path hints icons -->
                        <Grid Grid.Row="0" x:Name="HollowBastionPath" Grid.Column="1">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="0.18*" />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition Height="0.18*" />
                            </Grid.RowDefinitions>
                            <Image x:Name="HollowBastionPathCon" Source="Images/System/proof_connection.png"
                                   Visibility="Visible" Grid.Row="1" VerticalAlignment="Bottom" />
                            <Image x:Name="HollowBastionPathNon" Source="Images/System/proof_nonexistence.png"
                                   Visibility="Visible" Grid.Row="2" />
                            <Image x:Name="HollowBastionPathPea" Source="Images/System/proof_peace.png"
                                   Visibility="Visible" Grid.Row="3" VerticalAlignment="Top" />
                        </Grid>
                        <!-- item grid -->
                        <local:WorldGrid Grid.Row="0" x:Name="HollowBastionGrid" Grid.Column="2" />
