                                        <GradientStop Color="#FFC62937" Offset="0.927" />
                                        <GradientStop Color="#FFFF8B42" Offset="0.286" />
                                    </LinearGradientBrush>
                                </local:OutlinedTextBlock.Fill>
                            </local:OutlinedTextBlock>
                        </Viewbox>
                    </Grid>


                    <Border Name="DeathCounterRec" Grid.Column="1" Panel.ZIndex="-1" Margin="4,-6,-20,-0.5">
