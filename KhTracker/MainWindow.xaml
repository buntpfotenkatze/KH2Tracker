                            <fx:BrightnessContrastEffect Brightness="-1" />
                        </ContentControl.Effect>
                    </ContentControl>

                    <!-- alt silhouettes -->
                    <Image x:Name="Cross01" Source="Images/System/cross.png" Visibility="Collapsed" Grid.Column="0"
                           Panel.ZIndex="-2" Opacity="1">
                        <Image.Effect>
                            <fx:BrightnessContrastEffect Brightness="-1" />
                        </Image.Effect>
                    </Image>
                    <Image x:Name="Cross02" Source="Images/System/cross.png" Visibility="Collapsed" Grid.Column="1"
                           Panel.ZIndex="-2" Opacity="1">
                        <Image.Effect>
                            <fx:BrightnessContrastEffect Brightness="-1" />
                        </Image.Effect>
                    </Image>
                    <Image x:Name="Cross03" Source="Images/System/cross.png" Visibility="Collapsed" Grid.Column="2"
                           Panel.ZIndex="-2" Opacity="1">
                        <Image.Effect>
                            <fx:BrightnessContrastEffect Brightness="-1" />
                        </Image.Effect>
                    </Image>
                    <Image x:Name="Cross04" Source="Images/System/cross.png" Visibility="Collapsed" Grid.Column="3"
                           Panel.ZIndex="-2" Opacity="1">
                        <Image.Effect>
                            <fx:BrightnessContrastEffect Brightness="-1" />
                        </Image.Effect>
                    </Image>
                    <Image x:Name="Cross05" Source="Images/System/cross.png" Visibility="Collapsed" Grid.Column="4"
                           Panel.ZIndex="-2" Opacity="1">
                        <Image.Effect>
                            <fx:BrightnessContrastEffect Brightness="-1" />
                        </Image.Effect>
                    </Image>
                    <Image x:Name="Cross06" Source="Images/System/cross.png" Visibility="Collapsed" Grid.Column="5"
                           Panel.ZIndex="-2" Opacity="1">
                        <Image.Effect>
                            <fx:BrightnessContrastEffect Brightness="-1" />
                        </Image.Effect>
                    </Image>
                    <Image x:Name="Cross07" Source="Images/System/cross.png" Visibility="Collapsed" Grid.Column="6"
                           Panel.ZIndex="-2" Opacity="1">
                        <Image.Effect>
                            <fx:BrightnessContrastEffect Brightness="-1" />
                        </Image.Effect>
                    </Image>
                    <Image x:Name="Cross08" Source="Images/System/cross.png" Visibility="Collapsed" Grid.Column="7"
                           Panel.ZIndex="-2" Opacity="1">
                        <Image.Effect>
                            <fx:BrightnessContrastEffect Brightness="-1" />
                        </Image.Effect>
                    </Image>
                    <Image x:Name="Cross09" Source="Images/System/cross.png" Visibility="Collapsed" Grid.Column="8"
