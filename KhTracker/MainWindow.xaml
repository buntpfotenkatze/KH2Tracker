                                    <ScaleTransform ScaleX="0.75" ScaleY="1.25" />
                                    <SkewTransform />
                                    <RotateTransform Angle="8" />
                                    <TranslateTransform X="0" Y="-2" />
                                </TransformGroup>
                            </Viewbox.RenderTransform>
                            <local:OutlinedTextBlock x:Name="CollectedBar" FontFamily="{StaticResource Gummi}"
                                                     FontSize="20" FontStretch="Medium" AllowDrop="False"
                                                     IsHitTestVisible="False" StrokeThickness="1" Text="|">
                                <local:OutlinedTextBlock.Fill>
                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop Color="#FFE47606" Offset="0.836" />
                                        <GradientStop Color="#FFFFE435" Offset="0.237" />
                                    </LinearGradientBrush>
                                </local:OutlinedTextBlock.Fill>
