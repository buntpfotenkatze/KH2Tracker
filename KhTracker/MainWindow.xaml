                        <TextBlock FontFamily="{StaticResource Menu}" Foreground="White" FontSize="20pt">
                            <TextBlock.Effect>
                                <DropShadowEffect ShadowDepth="3" BlurRadius="2" Direction="320" />
                            </TextBlock.Effect>

                            <Run Name="HintTextBegin" />
                            <Run Name="HintTextMiddle" xml:space="preserve" />
                            <Run Name="HintTextEnd" xml:space="preserve" />
                        </TextBlock>

                    </Viewbox>

                    <!--Viewbox>
                        <TextBlock FontFamily="{StaticResource Menu}" FontWeight="DemiBold" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="0,0,-4,0" Foreground="White">
