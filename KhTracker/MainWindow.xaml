                                <local:OutlinedTextBlock.Fill>
                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop Color="#FFDB3735" Offset="1" />
                                        <GradientStop Color="#FFFF9647" Offset="0.4" />
                                    </LinearGradientBrush>
                                </local:OutlinedTextBlock.Fill>
                            </local:OutlinedTextBlock>
                        </Viewbox>
                    </Grid>
                    <!-- Magic -->
