                    <ContentControl Grid.Row="0" x:Name="LuckyEmblemsIcon" Content="{DynamicResource LuckyEmblem}"
                                    Grid.Column="12" HorizontalAlignment="Right" />

                    <!-- IC Count -->
                    <Grid Grid.Column="11" x:Name="CollectionGrid" Visibility="Visible">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="0*" />
                            <ColumnDefinition Width="1.15*" />
                            <ColumnDefinition Width="0.3*" />
                            <ColumnDefinition Width="1*" />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="0.15*" />
                            <RowDefinition Height="1*" />
                            <RowDefinition Height="0.05*" />
                        </Grid.RowDefinitions>
                        <!--ContentControl Content="{DynamicResource Min-Chest}" Name="ChestIcon"/-->
                        <!-- Collected -->
                        <Viewbox Grid.Row="1" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Stretch"
                                 Margin="0,0,0,3">
                            <Viewbox.RenderTransform>
                                <TransformGroup>
                                    <ScaleTransform />
                                    <SkewTransform />
                                    <RotateTransform />
                                    <TranslateTransform Y="0" X="-2" />
