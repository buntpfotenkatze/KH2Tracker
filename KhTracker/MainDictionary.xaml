<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ControlTemplate x:Key="DefaultButton" TargetType="{x:Type ButtonBase}">
        <ContentPresenter Content="{TemplateBinding Content}" />
    </ControlTemplate>

    <!--brush colors-->
    <LinearGradientBrush x:Key="ColorFire" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FFFF5A1A" Offset="1" />
        <GradientStop Color="#FFFFAA3B" Offset="0.4" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="ColorBlizzard" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FF6F50EF" Offset="1" />
        <GradientStop Color="#FFA4C8FF" Offset="0.4" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="ColorThunder" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FFE17606" Offset="1" />
        <GradientStop Color="#FFFFE437" Offset="0.4" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="ColorCure" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FF62B055" Offset="1" />
        <GradientStop Color="#FFECFF5B" Offset="0.4" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="ColorReflect" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FFACECFF" Offset="1" />
        <GradientStop Color="#FFE6FFFF" Offset="0.4" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="ColorMagnet" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FFFF58C0" Offset="1" />
        <GradientStop Color="#FFFFA5CA" Offset="0.4" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="ColorPage" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FFE16902" Offset="1" />
        <GradientStop Color="#FFFFF64A" Offset="0.4" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="ColorPouch" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FFD73435" Offset="1" />
        <GradientStop Color="#FFFF9546" Offset="0.4" />
    </LinearGradientBrush>
    <SolidColorBrush x:Key="ColorBlack" Color="Black" />
    <SolidColorBrush x:Key="ColorTrans" Color="Transparent" />

    <!--Classic-->
    <SolidColorBrush x:Key="ClassicYellow" Color="#FFE500" />
    <!--Hinted Hints-->
    <SolidColorBrush x:Key="HintedHint" Color="#29F" />
    <!--Ghosted Hints-->
    <SolidColorBrush x:Key="GhostHint" Color="#6D3" />
    <!--Progression Hinted-->
    <SolidColorBrush x:Key="ProgressionHinted" Color="#2a732e" />
    <!--Drive / AG / 100AW / Thunder / Torn Page / Master / Chicken Little / Olympus Stone / Scimitar / Puzzle-->
    <SolidColorBrush x:Key="Gold" Color="#FD0" />
    <!--DC / Wisdom / Stitch / Ice Cream / Once More-->
    <SolidColorBrush x:Key="LightBlue" Color="#5EF" />
    <!--PR / Blizzard / Anti-Form / Identity Disk Skill and Crossbones-->
    <SolidColorBrush x:Key="DarkBlue" Color="#8AF" />
    <!--OC / Defence / Cure / Battlefields of War / Second Chance-->
    <SolidColorBrush x:Key="Green" Color="#CF4" />
    <!--STT / GoA / Reflect / Proof of Nonexistence-->
    <SolidColorBrush x:Key="WhiteBlue" Color="#CFF" />
    <!--HT / Magic / Genie / Membership Card / Bone Fist-->
    <SolidColorBrush x:Key="Purple" Color="#D8F" />
    <!--TWTNW / Report / Final / Proof of Peace / Unknown Disc-->
    <SolidColorBrush x:Key="White" Color="#DDF" />
    <!--LoD / PL / Valor / Peter Pan / Proud Fang / Moogle-->
    <SolidColorBrush x:Key="Red" Color="#F63" />
    <!--HB / BC / Magnet / Beast’s Claw / Promise Charm / Moogle&Puzzle-->
    <SolidColorBrush x:Key="Pink" Color="#F7C" />
    <!--TT / Strength / Fire / Limit / Picture / Sword of the Ancestor-->
    <SolidColorBrush x:Key="Orange" Color="#F84" />
    <!--Proof of Connection-->
    <SolidColorBrush x:Key="OffWhite" Color="#FDC" />
    <!--default-->
    <SolidColorBrush x:Key="DefWhite" Color="White" />
    <!-- selection hilights -->
    <SolidColorBrush x:Key="DefaultWhite" Color="#FFEFECE9" />
    <SolidColorBrush x:Key="DefaultRec" Color="#D81E1E1E" />
    <SolidColorBrush x:Key="SelectedRec" Color="#D8466482" />
    <!-- Window Background default -->
    <SolidColorBrush x:Key="Bg-Default" Color="#303030" />
    <ImageBrush x:Key="Bg-ImageDef1" ImageSource="Images\System\background\final_mix.png" TileMode="Tile" />
    <ImageBrush x:Key="Bg-ImageDef2" ImageSource="Images\System\background\final_mix.png" TileMode="Tile"
                Stretch="UniformToFill" />
    <ImageBrush x:Key="Bg-ImageDef3" ImageSource="Images\System\background\final_mix.png" TileMode="Tile"
                Stretch="None" />

    <!--auto-detect icons-->
    <Image x:Key="Min-Connect" Source="Images\System\Config\searching.png" x:Shared="false" />
    <Image x:Key="Min-Pc" Source="Images\System\Config\pc_connected.png" x:Shared="false" />
    <Image x:Key="Min-PCred" Source="Images\System\Config\pc_detected.png" x:Shared="false" />
